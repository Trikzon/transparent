{
  "schema_version": 1,
  "quilt_loader": {
    "group": "com.diontryban.transparent",
    "id": "transparent",
    "version": "${version}",
    "metadata": {
      "name": "Transparent",
      "description": "Allows resource packs to make certain entities support transparency.",
      "contributors": {
        "Trikzon": "Owner"
      },
      "contact": {
        "homepage": "https://github.com/Trikzon",
        "issues": "https://github.com/Trikzon/transparent/issues",
        "sources": "https://github.com/Trikzon/transparent"
      },
      "icon": "assets/transparent/icon.png",
      "license": "LGPL-3.0-or-later"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "client_init": "com.diontryban.transparent.client.TransparentClientQuilt"
    },
    "depends": [
      {
        "id": "quilted_fabric_api",
        "versions": ">=7.0.0-"
      },
      {
        "id": "minecraft",
        "versions": "^1.20"
      },
      {
        "id": "ash",
        "versions": ">=2.0.0-"
      }
    ]
  },
  "access_widener": "transparent.accesswidener",
  "mixin": [
    "transparent.mixins.json",
    "transparent.quilt.mixins.json"
  ]
}
